{
    "telemetry.enableTelemetry": false,
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": 0.6,
    "editor.fontFamily": "'Cascadia Code', 'Fira Code', 'monospace', monospace, 'Droid Sans Fallback'",
    "platformio-ide.activateOnlyOnPlatformIOProject": true,
    "files.trimTrailingWhitespace": true,
    "workbench.editor.enablePreview": false,
    "editor.tabSize": 2,
    "editor.renderWhitespace": "boundary",
    "git.autofetch": true,
    "vim.easymotion": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.useSystemClipboard": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["f", "d"],
            "after": ["<Esc>"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
    ],
    "vim.leader": ",",
    "vim.surround": true,
    "workbench.editor.tabSizing": "shrink",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "editor.minimap.enabled": false,
    "search.runInExtensionHost": true,
    "ccls.launch.command": "~/.local/bin/ccls",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "platformio-ide.forceUploadAndMonitor": true,
    "workbench.colorTheme": "Base16 Dark Oceanic Next",
    "workbench.iconTheme": "vscode-great-icons",
    "editor.fontLigatures": true,
    "window.menuBarVisibility": "toggle"
}